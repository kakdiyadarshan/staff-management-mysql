<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Manage Task</title>
</head>
<body>
    <h2>Manage Task</h2>
    <table border="" style="text-align: center;">
        <tr>
            <th> Id</th>
            <th>Name With Id</th>
            <th>Task Name</th>
            <th>Starting Date</th>
            <th>Ending Date</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
        <% result.forEach(function(data){ %>
            <tr>
                <td>
                    <%= data.id%>
                </td>
                <td>
                    <%= data.staffname%>
                </td>
                <td>
                    <%= data.taskname%>
                </td>
                <td>
                    <%= data.startdate%>
                </td>
                <td>
                    <%= data.enddate%>
                </td>
                <td><a href="/manageedit/<%= data.id%>">Update</a></td>
                <td><a href="/managedel/<%= data.id%>">Delete</a></td>
            </tr>
            <% }); %>
    </table>
</body>
</html>